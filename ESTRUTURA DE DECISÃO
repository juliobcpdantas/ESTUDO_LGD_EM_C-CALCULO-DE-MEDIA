#include <ionstream>
using namespace std;
int main(int argc, char** argv) {
float nota1;
float nota2;
float media;

cout<<"Digite a primeira nota:"<<endl;
cin>>nota1;
cout<<"digite a segunda nota:"<<endl;
cin>>nota2;
media = (nota1+nota2)/2;

if(media>=6){
cout<<"Aprovado"<<endl;
}
cout<<"Excute novamente para calcular outra media"<<endl;
return 0;
}
